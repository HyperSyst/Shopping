@using Shopping.Models.Vnpay
@model PaymentResponseModel

@{
    ViewData["title"] = "Kết quả thông tin giao dịch thanh toán VnPay";
}
<h4>Thanh toán thành công với:</h4>
<h5>ID: @Model.OrderId</h5>
<h5>Thông tin: @Model.OrderDescription</h5>
<h5>Phương thức thanh toán: @Model.PaymentMethod</h5>
<h5>Mã giao dịch: @Model.TransactionId</h5>
<h5>Mã thanh toán: @Model.PaymentId</h5>
<h5>Xem <a asp-action="History" asp-controller="Account">lịch sử đơn hàng đã đặt</a></h5>